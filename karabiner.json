              {
                "description": "Toggle Hyper sublayer o",
                "type": "basic",
                "from": {
                  "key_code": "o",
                  "modifiers": {
                    "mandatory": [
                      "left_command",
                      "left_control",
                      "left_shift",
                      "left_option"
                    ]
                  }
                },
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_o",
                      "value": 0
                    }
                  }
                ],
                "to": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_o",
                      "value": 1
                    }
                  }
                ],
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_s",
                    "value": 0
                  },
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_v",
                    "value": 0
                  }
                ]
              },
