        "rules": [
          {
            "description": "Hyper Key (⌃⌥⇧⌘) when Tab is held",
            "manipulators": [
              {
                "description": "Tab Held -> Hyper Key",
                "from": {
                  "key_code": "tab"
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": [
                      "left_command",
                      "left_control",
                      "left_option"
                    ]
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "tab"
                  }
                ],
                "type": "basic"
              },
              {
                "type": "basic",
                "description": "Slash held -> Hyper Key",
                "from": {
                  "key_code": "slash"
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": [
                      "left_command",
                      "left_control",
                      "left_option"
                    ]
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "slash"
                  }
                ]
              }
            ]
          },
          {
            "description": "Hyper Key sublayer \"o\"",
